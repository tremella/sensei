
<%= form_for @slot do |form| %>

  <div class="field">
    <%= form.label :start %>
    <%= form.datetime_field :start %>
  </div>



  <div class="field">
    <%= form.label :coach %>
    <%= form.text_field :coach_id, readonly:true, value: @slot.coach_id %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>

<% end %>
